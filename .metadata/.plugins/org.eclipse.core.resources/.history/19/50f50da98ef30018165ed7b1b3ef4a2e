package StreamPractice;

import java.util.Arrays;
import java.util.List;
import java.util.TreeSet;
import java.util.stream.Collectors;

public class Stream1 {

	public static void main(String[] args) {
		
		List<String> list = Arrays.asList("Roshan", "Rosh","Rosha","Ros","Ro");
		
		List<String> newList = list.stream().map(name -> name+"kanwal").collect(Collectors.toList());
		TreeSet<String> set = (TreeSet<String>) list.stream().map(name -> name + name).collect(Collectors.toSet());
		
		System.out.println(set);
		System.out.println(newList);
	}
	
}
