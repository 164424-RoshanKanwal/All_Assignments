package com.lambda2;

import java.util.function.BiFunction;

public class LambdaQ2 {
	
	static int orderPrice;
	static String orderStatus;
	
	public LambdaQ2(int orderPrice, String orderStatus) {
		super();
		this.orderPrice = 10000;
		this.orderStatus = "Completed";
	}



	public static void main(String[] args) {
		
	BiFunction<Integer, String, Boolean> biFunction = (a,b) ->{
		if(orderPrice>=10000 && orderStatus.equalsIgnoreCase("Completed")){
			return true;
		}
		else{
			return false;
		}
	};
	}

}

	