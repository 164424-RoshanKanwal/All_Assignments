package com.lambda2;

import java.util.function.BiFunction;

public class LambdaQ2 {
	
	



	public static void main(String[] args) {
		
		
		
	BiFunction<Integer, String, Boolean> biFunction = (orderPrice,orderStatus) ->{
		if(orderPrice>=10000 && orderStatus.equalsIgnoreCase("Completed")){
			return true;
		}
		else{
			return false;
		}
	};
	System.out.println(biFunction.apply(23000, "completed"));
	
	}



	
		
	}
	
	


	