package com.roshan.collectionAssignmentQuestion9;

import java.util.LinkedHashSet;

public class BankAccountList {

	static LinkedHashSet<SavingAccount> linkedSet;
	public BankAccountList() {
		linkedSet = new LinkedHashSet<SavingAccount>();
	}
	
	public boolean addAcc(SavingAccount savingAcc){
		return linkedSet.add(savingAcc.getAccHolderName());
	}
	
	public static void main(String[] args) {
		
		BankAccountList accList = new BankAccountList();
		
		accList.addAcc(new SavingAccount(101, "Roshan", 1000, true));
		accList.addAcc(new SavingAccount(102, "Kanwal", 2343, false));
		
		System.out.println(linkedSet);
		
		
	}
	
	
	
}
